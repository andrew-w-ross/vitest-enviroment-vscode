{
	"name": "vitest-environment-vscode",
	"version": "0.1.1-0",
	"type": "module",
	"description": "A Vitest environment for VS Code",
	"main": "./dist/index",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index"
		},
		"./vscode-worker": {
			"types": "./dist/vscode-worker.d.ts",
			"import": "./dist/vscode-worker"
		},
		"./vscode-worker.cjs": {
			"require": "./vscode-worker.cjs"
		}
	},
	"files": [
		"dist",
		"vscode-worker.cjs"
	],
	"scripts": {
		"build": "vite build --mode development",
		"build:watch": "vite build --mode development --watch",
		"prerelease": "yarn build && yarn typecheck",
		"typecheck": "tsc --build",
		"typecheck:watch": "tsc --build --watch",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"keywords": [
		"vitest",
		"vscode",
		"environment"
	],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@repo/typescript-configs": "workspace:*",
		"@types/node": "^24.8.1",
		"@types/vscode": "^1.106.1",
		"@types/ws": "^8.18.1",
		"typescript": "^5.9.3",
		"vite": "^7.2.4",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^4.0.13"
	},
	"peerDependencies": {
		"vitest": ">=3.2.4"
	},
	"dependencies": {
		"@vscode/test-electron": "^2.5.2",
		"core-js": "^3.47.0",
		"devalue": "^5.5.0",
		"indisposed": "^0.0.7",
		"type-fest": "^5.2.0",
		"ws": "^8.18.3",
		"zod": "^4.1.12"
	},
	"stableVersion": "0.1.0"
}
